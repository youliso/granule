{
  "name": "@youliso/web-modules",
  "version": "1.0.0",
  "description": "web modules vanilla",
  "scripts": {
    "build": "rollup -c rollup.config.mjs && node build.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "fix:prettier": "prettier \"./src\" --write",
    "test:prettier": "prettier \"./src\" --list-different"
  },
  "keywords": [
    "web",
    "vanilla",
    "ts",
    "jsx"
  ],
  "author": "youliso",
  "repository": {
    "type": "git",
    "url": "https://github.com/youliso/web-modules"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {},
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.10",
    "prettier": "^2.8.0",
    "rollup": "^3.5.1",
    "rollup-plugin-esbuild-minify": "^1.0.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3"
  }
}
